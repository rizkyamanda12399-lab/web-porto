// schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // gunakan variabel .env
}

model Message { 
  id        Int      @id @default(autoincrement())
  name      String
  subject   String
  message   String
  createdAt DateTime @default(now())
}
